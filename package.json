{
  "name": "pages-builder",
  "repository": {
    "type": "git",
    "url": "git://github.com/cloud-gov/pages-builder.git"
  },
  "version": "0.0.0",
  "private": true,
  "description": "Checks a Redis queue and launches cloud.gov Pages builds",
  "main": "app.js",
  "scripts": {
    "dev": "node -r .env app",
    "start": "node app",
    "test": "NODE_ENV=test mocha --require test/env.js --exit",
    "test:cover": "NODE_ENV=test nyc mocha --require test/env.js --exit",
    "eslint": "eslint ."
  },
  "author": "dhcole",
  "license": "CC0-1.0",
  "dependencies": {
    "@hapi/hapi": "^20.1.3",
    "aws-sdk": "^2.907.0",
    "axios": "^1.6.5",
    "bull": "^3.22.5",
    "cfenv": "^1.2.4",
    "jsonwebtoken": "^9.0.0",
    "newrelic": "^8.9.1",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "dotenv": "^16.0.1",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "mocha": "^10.0.0",
    "nock": "^13.0.11",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^14.0.0"
  },
  "engines": {
    "node": "18.x.x",
    "npm": "8.x.x"
  },
  "nyc": {
    "exclude": [
      "test/**/*.js"
    ],
    "reporter": [
      "lcov"
    ]
  }
}
